{
  "name": "rahimov/yii2-filemanager-php",
  "description": "Filemanager for yii2, it is the connector to the File Manager, regardless of the GUI.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "filemanager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Rahimov",
      "email": "a.rahimov@me.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/moment": "~2.9.0",
    "bower-asset/bootstrap-contextmenu": "*",
    "bower-asset/filemanager-ui":  "*",
    "guillermomartinez/filemanager-php": "*"
  },
  "autoload": {
    "psr-4": {
      "rahimov\\filemanager\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
